.loginPopup-box {
  width: 550rpx;
  height: 400rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.loginPopup-box .title {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 32rpx;
}
.loginPopup-box .subtitle {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 28rpx;
  color: #a1a1a1;
  margin-top: 32rpx;
}
.loginPopup-box .btn{
  width: 400rpx;
  margin-top: 100rpx;
}